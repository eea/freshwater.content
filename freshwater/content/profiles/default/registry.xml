<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="plone">


  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.legislation">
    <value key="title" i18n:translate="">Legislation</value>
    <value key="description" i18n:translate=""></value>
    <value key="enabled">True</value>
    <value key="sortable">True</value>
    <value key="operations">
      <element>plone.app.querystring.operation.selection.any</element>
      <element>plone.app.querystring.operation.selection.all</element>
    </value>
    <value key="group" i18n:translate="">Freshwater</value>
    <value key="vocabulary">wise_legislative_vocabulary</value>
  </records>

  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.category">
    <value key="title" i18n:translate="">Sub-Theme</value>
    <value key="description" i18n:translate=""></value>
    <value key="enabled">True</value>
    <value key="sortable">True</value>
    <value key="operations">
      <element>plone.app.querystring.operation.selection.any</element>
      <element>plone.app.querystring.operation.selection.all</element>
    </value>
    <value key="group" i18n:translate="">Freshwater</value>
    <value key="vocabulary">wise_category_vocabulary</value>
  </records>

  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.dpsir_type">
    <value key="title" i18n:translate="">DPSIR</value>
    <value key="description" i18n:translate=""></value>
    <value key="enabled">True</value>
    <value key="sortable">True</value>
    <value key="operations">
      <element>plone.app.querystring.operation.selection.any</element>
      <element>plone.app.querystring.operation.selection.all</element>
    </value>
    <value key="group" i18n:translate="">Freshwater</value>
    <value key="vocabulary">wise_dpsir_vocabulary</value>
  </records>

  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.country_code">
    <value key="title" i18n:translate="">Country Code</value>
    <value key="description" i18n:translate=""></value>
    <value key="enabled">True</value>
    <value key="sortable">True</value>
    <value key="operations">
      <element>plone.app.querystring.operation.selection.any</element>
    </value>
    <value key="group" i18n:translate="">DataConnector Parameter</value>
    <value key="vocabulary">wise_countries_complete_vocabulary</value>
  </records>

  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.taxonomy_eeatopicstaxonomy" remove="true">
  </records>

  <records interface="plone.app.querystring.interfaces.IQueryField"
    prefix="plone.app.querystring.field.taxonomy_eeapublishertaxonomy" remove="true">
  </records>

  <record name="plone.allowed_sizes" interface="plone.app.imaging.interfaces.IImagingSchema" field="allowed_sizes">
    <field type="plone.registry.field.List">
      <description>Specify all allowed maximum image dimensions, one per line. The required format is &lt;name&gt; &lt;width&gt;:&lt;height&gt;.</description>
      <required>False</required>
      <title>Allowed image sizes</title>
      <value_type type="plone.registry.field.TextLine"/>
    </field>
    <value>
      <element>huge 1600:65536</element>
      <element>icon 32:32</element>
      <element>tile 64:64</element>
      <element>thumb 128:128</element>
      <element>mini 200:65536</element>
      <element>preview 400:65536</element>
      <element>teaser 600:65536</element>
      <element>large 800:65536</element>
      <element>larger 1000:65536</element>
      <element>great 1200:65536</element>
      <element>listing 16:16</element>
      <element>panoramic 1920:1080</element>
    </value>
  </record>

</registry>
